{
  "name": "ontology-ts-test",
  "version": "0.2.3",
  "description": "",
  "bin": {
    "ontology-ts-test": "./lib/ontology-ts-test.js"
  },
  "main": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project ./",
    "copy-cli": "copyfiles -f ./src/cli/ontology-ts-test.js ./lib",
    "partBuild:dev": "cross-env NODE_ENV=development webpack --mode development --display-error-details --progress --color",
    "partBuild:prod": "cross-env NODE_ENV=production webpack --mode production --progress --color",
    "build:dev": "npm-run-all -s partBuild:dev copy-cli",
    "build:prod": "npm-run-all -s partBuild:prod copy-cli"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.test\\.(ts|tsx|js)$",
    "moduleDirectories": [
      "<rootDir>/node_modules"
    ]
  },
  "author": "Matus Zamborsky <zamborsky@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/OntologyCommunityDevelopers/ontology-ts-cli"
  },
  "bugs": {
    "url": "https://github.com/OntologyCommunityDevelopers/ontology-ts-test/issues"
  },
  "homepage": "https://github.com/OntologyCommunityDevelopers/ontology-ts-test",
  "devDependencies": {
    "@types/bs58": "^3.0.30",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "jest": "^23.4.2",
    "jest-cli": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-no-circular-imports": "^0.4.0",
    "typescript": "^3.0.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/long": "~4",
    "commander": "^2.18.0",
    "cross-fetch": "^2.2.2",
    "long": "~4",
    "ontology-ts-crypto": "^0.1.10"
  }
}
